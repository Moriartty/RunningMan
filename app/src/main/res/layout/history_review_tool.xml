<?xml version="1.0" encoding="utf-8"?>
<android.support.v4.widget.NestedScrollView
    xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context=".Activity.HistoryDataDisplay">
    <LinearLayout
        android:layout_height="match_parent"
        android:layout_width="match_parent"
        android:orientation="vertical"
        android:gravity="center_horizontal">

        <LinearLayout
            android:orientation="vertical"
            android:layout_width="match_parent"
            android:layout_height="match_parent"
            android:gravity="center_horizontal">
            <com.moriarty.user.runningman.User_Defind.Fuck.NoScrollViewPager
                android:id="@+id/walkstep_history_show"
                android:layout_width="match_parent"
                android:layout_height="200dp" />
            <android.support.design.widget.TabLayout
                android:id="@+id/walkstep_history_selector"
                app:tabSelectedTextColor="@color/darkslategray"
                app:tabIndicatorColor="@color/darkslategray"
                android:layout_width="match_parent"
                android:layout_height="40dp" />
        </LinearLayout>

        <LinearLayout
            android:orientation="vertical"
            android:layout_width="match_parent"
            android:layout_height="match_parent"
            android:gravity="center_horizontal"
            >
            <com.moriarty.user.runningman.User_Defind.Fuck.NoScrollViewPager
                android:id="@+id/walkdistance_history_show"
                android:layout_width="match_parent"
                android:layout_height="200dp" />
            <android.support.design.widget.TabLayout
                android:id="@+id/walkdistance_history_selector"
                app:tabSelectedTextColor="@color/darkslategray"
                app:tabIndicatorColor="@color/darkslategray"
                android:layout_width="match_parent"
                android:layout_height="40dp" />
        </LinearLayout>

        <LinearLayout
            android:orientation="vertical"
            android:layout_width="match_parent"
            android:layout_height="match_parent"
            android:gravity="center_horizontal"
            >
            <com.moriarty.user.runningman.User_Defind.Fuck.NoScrollViewPager
                android:id="@+id/rundistance_history_show"
                android:layout_width="match_parent"
                android:layout_height="200dp" />
            <android.support.design.widget.TabLayout
                android:id="@+id/rundistance_history_selector"
                app:tabSelectedTextColor="@color/darkslategray"
                app:tabIndicatorColor="@color/darkslategray"
                android:layout_width="match_parent"
                android:layout_height="40dp" />
        </LinearLayout>

        <LinearLayout
            android:orientation="vertical"
            android:layout_width="match_parent"
            android:layout_height="match_parent"
            android:gravity="center_horizontal"
            >
            <com.moriarty.user.runningman.User_Defind.Fuck.NoScrollViewPager
                android:id="@+id/ridedistance_history_show"
                android:layout_width="match_parent"
                android:layout_height="200dp" />
            <android.support.design.widget.TabLayout
                android:id="@+id/ridedistance_history_selector"
                app:tabSelectedTextColor="@color/darkslategray"
                app:tabIndicatorColor="@color/darkslategray"
                android:layout_width="match_parent"
                android:layout_height="40dp" />
        </LinearLayout>

        <LinearLayout
            android:orientation="vertical"
            android:layout_width="match_parent"
            android:layout_height="match_parent"
            android:gravity="center_horizontal"
            >
            <com.moriarty.user.runningman.User_Defind.Fuck.NoScrollViewPager
                android:id="@+id/calories_history_show"
                android:layout_width="match_parent"
                android:layout_height="200dp" />
            <android.support.design.widget.TabLayout
                android:id="@+id/calories_history_selector"
                app:tabSelectedTextColor="@color/darkslategray"
                app:tabIndicatorColor="@color/darkslategray"
                android:layout_width="match_parent"
                android:layout_height="40dp" />
        </LinearLayout>
    </LinearLayout>
</android.support.v4.widget.NestedScrollView>